{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "max-len": [
            "error",
            275,
            4
        ],
        "no-confusing-arrow": 0,
        "react/jsx-quotes": 0,
        "react/prefer-stateless-function": 0,
        "jsx-quotes": [
            2,
            "prefer-double"
        ],
        "no-underscore-dangle": 0,
        "import/no-unresolved": 0
    },
    "env": {
        "es6":true,
        "node":true
    }
}